{
    "name": "IONOS Cloud Control",
    "id": "ionosctl",
    "version": "1.1.0",
    "description": "A feature to install IONOS cloud control 'ionosctl'",
    "options": {
        "version": {
            "type": "string",
            "default": "latest",
            "description": "Choose the version."
        },
        "sha256": {
            "type": "string",
            "default": "",
            "description": "Optional hash for validation"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils",
        "ghcr.io/devcontainers-contrib/features/curl-apt-get"
    ]
}
